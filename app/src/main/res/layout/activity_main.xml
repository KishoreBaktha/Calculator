<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kishorebaktha.calculator.MainActivity">

    <Button
        android:id="@+id/b1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="135dp"
        android:layout_above="@+id/b2"
        android:layout_alignLeft="@+id/b2"
        android:layout_alignStart="@+id/b2"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp" />

    <Button
        android:id="@+id/b2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="135dp"
        android:layout_above="@+id/b5"
        android:layout_alignLeft="@+id/b5"
        android:layout_alignStart="@+id/b5" />

    <Button
        android:id="@+id/b3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        tools:layout_editor_absoluteX="199dp"
        tools:layout_editor_absoluteY="135dp"
        android:layout_above="@+id/b6"
        android:layout_alignLeft="@+id/b6"
        android:layout_alignStart="@+id/b6" />

    <Button
        android:id="@+id/b4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/b6"
        android:layout_alignLeft="@+id/b7"
        android:layout_alignStart="@+id/b7"
        android:text="4"
        tools:layout_editor_absoluteX="296dp"
        tools:layout_editor_absoluteY="135dp" />

    <Button
        android:id="@+id/b5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="192dp"
        android:layout_alignBaseline="@+id/b6"
        android:layout_alignBottom="@+id/b6"
        android:layout_alignLeft="@+id/b8"
        android:layout_alignStart="@+id/b8" />

    <Button
        android:id="@+id/b6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="192dp"
        android:layout_above="@+id/b9"
        android:layout_alignLeft="@+id/b9"
        android:layout_alignStart="@+id/b9"
        android:layout_marginBottom="11dp" />

    <Button
        android:id="@+id/b7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="192dp"
        android:layout_above="@+id/b0"
        android:layout_alignLeft="@+id/b0"
        android:layout_alignStart="@+id/b0" />

    <Button
        android:id="@+id/b8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        tools:layout_editor_absoluteX="296dp"
        tools:layout_editor_absoluteY="192dp"
        android:layout_alignBaseline="@+id/b9"
        android:layout_alignBottom="@+id/b9"
        android:layout_alignLeft="@+id/bpl"
        android:layout_alignStart="@+id/bpl" />

    <Button
        android:id="@+id/b9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="251dp"
        android:layout_alignBaseline="@+id/b0"
        android:layout_alignBottom="@+id/b0"
        android:layout_alignLeft="@+id/bmin"
        android:layout_alignStart="@+id/bmin" />

    <Button
        android:id="@+id/b0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="251dp"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/bmul"
        android:layout_alignStart="@+id/bmul" />

    <Button
        android:id="@+id/bpl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="251dp"
        android:layout_alignBaseline="@+id/bmin"
        android:layout_alignBottom="@+id/bmin"
        android:layout_alignRight="@+id/et"
        android:layout_alignEnd="@+id/et" />

    <Button
        android:id="@+id/bmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        tools:layout_editor_absoluteX="296dp"
        tools:layout_editor_absoluteY="251dp"
        android:layout_alignBaseline="@+id/bmul"
        android:layout_alignBottom="@+id/bmul"
        android:layout_alignLeft="@+id/beq"
        android:layout_alignStart="@+id/beq" />

    <Button
        android:id="@+id/bmul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="313dp"
        android:layout_above="@+id/bcl"
        android:layout_alignLeft="@+id/bcl"
        android:layout_alignStart="@+id/bcl"
        android:layout_marginBottom="20dp" />

    <Button
        android:id="@+id/bdiv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="307dp"
        android:layout_alignBaseline="@+id/beq"
        android:layout_alignBottom="@+id/beq"
        android:layout_alignLeft="@+id/bpl"
        android:layout_alignStart="@+id/bpl" />

    <Button
        android:id="@+id/beq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="="
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="313dp"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="113dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/et"
        android:layout_alignStart="@+id/et" />

    <Button
        android:id="@+id/bcl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C"
        tools:layout_editor_absoluteX="296dp"
        tools:layout_editor_absoluteY="313dp"
        android:layout_alignBaseline="@+id/beq"
        android:layout_alignBottom="@+id/beq"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp" />

    <EditText
        android:id="@+id/et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="47dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/b4"
        android:layout_toEndOf="@+id/b4"
        android:layout_marginTop="17dp" />

</RelativeLayout>
